{
    "name": "@kurlytail/sgen-bot",
    "description": "sgen-bot",
    "version": "0.0.593",
    "main": "dist/app.min.js",
    "devDependencies": {
        "child_process": "1.0.2",
        "circular-dependency-plugin": "5.2.2",
        "eslint": "7.30.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-babel-module": "5.3.1",
        "eslint-loader": "4.0.2",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-flowtype": "5.8.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jest": "24.3.6",
        "eslint-plugin-prettier": "3.1.4",
        "flow-bin": "0.155.0",
        "prettier": "2.3.2",
        "source-map-loader": "3.0.0",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "5.44.0",
        "webpack-cli": "4.7.2",
        "webpack-node-externals": "3.0.0",
        "jest": "27.0.6",
        "jest-junit-reporter": "1.1.0",
        "jest-localstorage-mock": "2.4.14",
        "@babel/cli": "7.14.5",
        "@babel/core": "7.14.6",
        "@babel/plugin-proposal-class-properties": "7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "7.14.7",
        "@babel/preset-env": "7.14.7",
        "@babel/preset-flow": "7.14.5",
        "@babel/register": "7.14.5",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "27.0.6",
        "babel-loader": "8.2.2",
        "babel-plugin-module-resolver": "4.1.0",
        "babel-preset-flow": "6.23.0",
        "@octokit/rest": "18.6.7"
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --mode=development --display-modules --display-reasons; chmod +x dist/app.min.js",
        "build-prod": "NODE_ENV=\"production\" webpack --mode=production --display-modules --display-reasons; chmod +x dist/app.min.js",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml",
        "gen": "sgen -g npm -g log -g bst -d design.json"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src"
        ],
        "testPathIgnorePatterns": [
            "./.generated"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/**/*.js",
            "!**/node_modules/**"
        ],
        "setupFiles": [
            "jest-localstorage-mock"
        ]
    },
    "dependencies": {
        "fs-extra": "^10.0.0",
        "logform": "2.2.0",
        "tmp": "^0.2.0",
        "winston": "3.3.3",
        "yaml": "^1.6.0"
    },
    "bin": {
        "sgen-bot": "dist/app.min.js"
    }
}
